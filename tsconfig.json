{
  "compilerOptions": {
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "target": "es2015",
    "module": "es2015",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable",
      "ES2021.WeakRef",
      "scripthost"
    ],
    "jsx": "react",
    "strict": true,
    "pretty": true,
    "newLine": "LF",
    "noImplicitThis": true,
    "noImplicitReturns": false,
    "types": ["jest", "node", "./apollo-client/matchers.d.ts"]
  }
}
